             xmlns:tffa="https://tffauto.no/xml/TFFAuto/2017-06-06/TFFAuto.xsd"
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">

  <documentation>
    Tjenestespesifikasjon for TFFAuto.

    © 2017 Trafikkforsikringsforeningen
    Forfatter: Dag Lem &lt;dag@nimrod.no&gt;
  </documentation>

  <types>
    <schema xmlns="http://www.w3.org/2001/XMLSchema">
      <import
        namespace="https://tffauto.no/xml/TFFAuto/2017-06-06/TFFAuto.xsd"
        schemaLocation="https://tffauto.no/xml/TFFAuto/2017-06-06/TFFAuto.xsd"/>
    </schema>
  </types>

  <message name="spoerring">
    <part name="Autentisering" element="tffa:Autentisering"/>
